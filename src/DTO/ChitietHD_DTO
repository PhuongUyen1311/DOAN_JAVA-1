
package DTO;

public class ChitietHD_DTO {
    private String maHD, maSP, maSize;
    private int sl;
    public ChitietHD_DTO() {
    }

    public ChitietHD_DTO(String maHD, String maSP, String maSize, int sl) {
        this.maHD = maHD;
        this.maSP = maSP;
        this.maSize = maSize;
        this.sl = sl;
    }

    public String getMaHD() {
        return maHD;
    }

    public String getMaSP() {
        return maSP;
    }

    public String getMaSize() {
        return maSize;
    }

    public int getSl() {
        return sl;
    }

    public void setMaHD(String maHD) {
        this.maHD = maHD;
    }

    public void setMaSP(String maSP) {
        this.maSP = maSP;
    }

    public void setMaSize(String maSize) {
        this.maSize = maSize;
    }

    public void setSl(int sl) {
        this.sl = sl;
    }
    
}
