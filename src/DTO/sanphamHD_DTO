package DTO;

public class sanphamHD_DTO {
    private String tensp, size;
    private int sl;
    private double gia, tt;

    public sanphamHD_DTO(String tensp, String size, int sl, double gia, double tt) {
        this.tensp = tensp;
        this.size = size;
        this.sl = sl;
        this.gia = gia;
        this.tt = tt;
    }

    public String getTensp() {
        return tensp;
    }

    public String getSize() {
        return size;
    }

    public int getSl() {
        return sl;
    }

    public double getGia() {
        return gia;
    }

    public double getTt() {
        return tt;
    }

    public void setTensp(String tensp) {
        this.tensp = tensp;
    }

    public void setSize(String size) {
        this.size = size;
    }

    public void setSl(int sl) {
        this.sl = sl;
    }

    public void setGia(double gia) {
        this.gia = gia;
    }

    public void setTt(double tt) {
        this.tt = tt;
    }
    
}
